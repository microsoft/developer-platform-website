{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Start",
            "type": "msedge",
            "cleanUp": "onlyTab",
            "request": "launch",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/src",
            "sourceMapPathOverrides": {
                "webpack:///src/*": "${webRoot}/*"
            },
            "preLaunchTask": "npm start",
        }
    ]
}